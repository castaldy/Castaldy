#include <iostream>
#include <math.h>

using namespace std;

int main ()
{
	int a, resultat, valAuxilliaire;
	
	cout << "entrer le nombre a: " << endl;
	cin >> a;
	
	valAuxilliaire = pow(a, 3); // a au cube = a^3
	
	resultat = valAuxilliaire * valAuxilliaire  * valAuxilliaire  * valAuxilliaire  * valAuxilliaire;
	// a^15 = a^3 * a^3 * a^3 * a^3 * a^3  
	
    cout << "le resultat est "  << resultat << " pour a = " << a << endl;
}
