#include<stdio.h>
#include<math.h>
 int main(){
 	int a,b,c,t; // integer
 	printf("Enter a,b,c");
 	scanf("%d%d%d", &a, &b, &c);
 	t = (a*b)/pow(c,2);
 	printf("Number of squares placed on the rectangle = %d", t);
 	printf("The area of the unoccupied part of the rectangle = %d", (a*b)-pow(c,2)*t);
 	
 	return 0;
 }
